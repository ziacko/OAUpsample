# Alternative GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug_linux
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug_linux)
  Example_config = debug_linux

else ifeq ($(config),release_linux)
  Example_config = release_linux

else
  $(error "invalid configuration $(config)")
endif

PROJECTS := Example

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

Example:
ifneq (,$(Example_config))
	@echo "==== Building Example ($(Example_config)) ===="
	@${MAKE} --no-print-directory -C . -f Example.make config=$(Example_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f Example.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug_linux"
	@echo "  release_linux"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   Example"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"